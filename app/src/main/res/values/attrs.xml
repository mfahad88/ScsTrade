<resources>
    <attr name="colorDarkerr" format="color" />
    <attr name="colorOnDarkerr" format="color" />
    <attr name="colorDarkerrContainer" format="color" />
    <attr name="colorOnDarkerrContainer" format="color" />
    <attr name="colorSpecialHeadingColor" format="color" />
    <attr name="colorOnSpecialHeadingColor" format="color" />
    <attr name="colorSpecialHeadingColorContainer" format="color" />
    <attr name="colorOnSpecialHeadingColorContainer" format="color" />
    <declare-styleable name="LabelledTextField">
        <attr name="hintText" format="string"/>
        <attr name="hintTextField" format="string"/>
        <attr name="android:inputType" format="reference"/>
        <attr name="passwordToggleEnabled" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="mLineChart">
        <attr name="isTouchEnabled" format="boolean"/>
        <attr name="isPinchZoom" format="boolean"/>
        <attr name="isDragEnabled" format="boolean"/>
        <attr name="isScaleEnabled" format="boolean"/>
        <attr name="isRightAxis" format="boolean"/>
        <attr name="isLeftAxis" format="boolean"/>
        <attr name="entries" format="string"/>
    </declare-styleable>
    <declare-styleable name="VolumeChip">
        <attr name="android:text" format="string|reference"/>
        <attr name="android:background" format="color|reference"/>
    </declare-styleable>

    <declare-styleable name="NetChangeChip">
        <attr name="android:text" format="string|reference"/>
        <attr name="android:background" format="color|reference"/>
    </declare-styleable>
    <declare-styleable name="HighView">
        <attr name="android:text" format="string|reference"/>
    </declare-styleable>
    <declare-styleable name="LowView">
        <attr name="android:text" format="string|reference"/>
    </declare-styleable>
    <declare-styleable name="TradeValueView">
        <attr name="android:text" format="string|reference"/>
        <attr name="android:src" format="reference"/>
    </declare-styleable>
    <declare-styleable name="TimeChip">
        <attr name="android:text" format="string|reference"/>
        <attr name="setSelected" format="boolean"/>
    </declare-styleable>
</resources>
